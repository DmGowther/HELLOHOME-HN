{
    "builds": [
        {
            "src": "src/**/*",
            "use": "@astrojs/vercel"
        },
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ]
}